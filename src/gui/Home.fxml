<AnchorPane maxHeight="400.0" maxWidth="500.0" minHeight="500.0" minWidth="300.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.HomeController">
   <children>
      <GridPane layoutX="-3.0" layoutY="-4.0" maxHeight="400.0" maxWidth="400.0" prefHeight="727.0" prefWidth="855.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="mainMsg" editable="false" text="Hello!" />
            <Button fx:id="createCourseBtn" mnemonicParsing="false" onAction="#createCourse" text="Create Course" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Hyperlink fx:id="logout" layoutX="432.0" layoutY="14.0" onAction="#homeExit" text="Logout" />
      <Button layoutX="383.0" layoutY="13.0" mnemonicParsing="false" onAction="#editProfile" text="Edit" />
